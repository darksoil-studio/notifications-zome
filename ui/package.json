{
  "name": "@darksoil-studio/notifications-zome",
  "version": "0.1.0",
  "scripts": {
    "start": "vite --clearScreen false --port $UI_PORT",
    "build": "pnpm lint && tsc && pnpm analyze",
    "lint": "eslint src && prettier \"src/**/*.ts\" --check --ignore-path .gitignore",
    "format": "eslint src --fix && prettier \"src/**/*.ts\" --write --ignore-path .gitignore",
    "localize:extract": "lit-localize extract",
    "localize:build": "lit-localize build",
    "analyze": "cem analyze --litelement --exclude dist",
    "prepare": "tsc && pnpm localize:build && pnpm analyze"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "locales",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/darksoil-studio/notifications-zome.git"
  },
  "exports": {
    ".": "./dist/index.js",
    "./dist/*": "./dist/*",
    "./locales/*": "./locales/*"
  },
  "dependencies": {
    "@darksoil-studio/linked-devices-zome": "github:darksoil-studio/linked-devices-zome#5eb4af940554974d61c65b90636999fcd0b54662&path:ui",
    "@darksoil-studio/profiles-zome": "github:darksoil-studio/profiles-zome#8e608fd6e094c4674631e79a0fba8276b93116dd&path:ui",
    "@tnesh-stack/elements": "^0.400.0-rc",
    "@tnesh-stack/signals": "^0.400.0-rc",
    "@tnesh-stack/utils": "^0.400.0-rc",
    "@holochain/client": "^0.18.0-rc",
    "@lit/context": "^1.0.0",
    "@lit/localize": "^0.12.0",
    "@mdi/js": "^7.2.0",
    "@msgpack/msgpack": "^2.8.0",
    "@shoelace-style/shoelace": "^2.11.0",
    "lit": "^3.0.0"
  },
  "devDependencies": {
    "@custom-elements-manifest/analyzer": "^0.9.4",
    "@lit/localize-tools": "^0.6.3",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5",
    "vite": "^4.0.0",
    "vite-plugin-checker": "^0.5.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "customElements": "custom-elements.json",
  "type": "module"
}